{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "jira-ticket-to-url",
  "title": "Jira Ticket to URL",
  "description": "Convert a Jira ticket (e.g. SF-8088) into a URL (e.g. https://mycompany.atlassian.net/browser/SF-8088).",
  "icon": "command-icon.png",
  "author": "salcode",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Jira Ticket to URL",
      "subtitle": "Convert Jira ticket (SF-8088) into a URL (https://mycompany.atlassian.net/browser/SF-8088)",
      "description": "Convert Jira Ticket to URL",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.66.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "preferences": [
    {
      "description": "Base URL for Jira links",
      "name": "baseurl",
      "placeholder": "https://mycompany.atlassian.net/",
      "required": true,
      "title": "Jira Base URL",
      "type": "textfield"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
